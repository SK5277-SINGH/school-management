<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="view_student_result" model="ir.ui.view">
			<field name="name">view.student.result</field>
			<field name="model">student.result</field>
			<field name="arch" type="xml">
				<form string="Result">
					<sheet>
						<group>
							<group>
								<field name="student_id"/>
								<field name="standard_id"/>
							</group>
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record id="tree_student_result" model="ir.ui.view">
			<field name="name">tree.student.result</field>
			<field name="model">student.result</field>
			<field name="arch" type="xml">
				<tree string="Result">
					<field name="student_id"/>
					<field name="standard_id"/>
				</tree>
			</field>
		</record>

		<record id="action_result" model="ir.actions.act_window">
			<field name="name">Result</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">student.result</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
		</record>

		<menuitem id="menu_result_view" name="Result" sequence="3" action="action_result" parent="menu_school"/>
	</data>
</odoo>
